{
  "name": "fssf",
  "version": "1.0.1",
  "description": "Promise based wrapper for node:fs",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./finishBuild",
    "test": "jest",
    "bnt": "npm run build && npm run test",
    "preversion": "npm test",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "scan": "node .scan.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adotland/fssf.git"
  },
  "keywords": [
    "fs",
    "csv",
    "json"
  ],
  "author": "adotland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adotland/fssf/issues"
  },
  "homepage": "https://github.com/adotland/fssf#readme",
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "auto-changelog": "^2.4.0",
    "jest": "^28.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14.14.0",
    "npm": ">=5.5"
  }
}
